    "packagewin64": "electron-packager ./app/ RuffHelper --platform=win32 --arch=x64 --out release --mirror http://localhost:3000/electronzip/  --version 0.37.5 --overwrite  --icon=./app/icon/icon.ico",
    "packagewin32": "electron-packager ./app/ RuffHelper --platform=win32 --arch=ia32 --out release --mirror http://localhost:3000/electronzip/  --version 0.37.5 --overwrite  --icon=./app/icon/icon.ico",
    "packagemac": "electron-packager ./app/ RuffHelper --platform=darwin --arch=x64 --out release --mirror http://localhost:3000/electronzip/  --version 0.37.5 --overwrite  --icon=./app/icon/icon.icns",
    "packagelinux": "electron-packager ./app/ RuffHelper --platform=linux --arch=x64 --out release --mirror http://localhost:3000/electronzip/  --version 0.37.5 --overwrite  --icon=./app/icon/icon.icns",
    "asar":"asar pack app app.asar",
    "start": "electron ."
